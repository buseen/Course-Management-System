package project;

import java.util.ArrayList;

public class Course{
	
	public int CourseId;
	public String CourseName;
	
	/*A course has many registered students*/
	public ArrayList<Student> students;
	
	public Course(int courseId, String courseName, ArrayList<Student> students) {
		super();
		CourseId = courseId;
		CourseName = courseName;
		this.students = students;
	}

	
	/*A course has many registered students*/

	
	
	
	
	public void passingStates(){
		/*display them*/
		for(Student st : students){
			System.out.print(st + " ");
			st.showPassingState();
		}
	}




	
		
	
	
	

}

